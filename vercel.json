{
	"builds": [
		{
			"src": "api/index.js",
			"use": "@vercel/node"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	],
	"buildCommand": "pnpm run build",
	"devCommand": "pnpm run dev",
	"installCommand": "pnpm install",
	"framework": null,
	"outputDirectory": "build",
	"functions": {
		"api/**/*.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	}
}
