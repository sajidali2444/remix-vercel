{
	"builds": [
		{
			"src": "api/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/api/index.js"
		}
	],
	"buildCommand": "pnpm run build",
	"devCommand": "pnpm run dev",
	"installCommand": "pnpm install",
	"framework": "remix",
	"outputDirectory": "build/client",
	"functions": {
		"api/**/*.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api"
		}
	]
}
